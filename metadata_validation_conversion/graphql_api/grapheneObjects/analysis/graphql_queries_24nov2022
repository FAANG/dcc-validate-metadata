# query{
#   allDatasets(
#     filter:{
#       basic:{
#         accession:"PRJEB14330" #ChIPSeqDnaBinding:PRJEB35307 HiC:PRJEB27364 rnaseq:PRJEB14330  cagesq:PRJEB34864 atacseq:PRJEB27111 bsSeq:PRJEB9561
#       }
